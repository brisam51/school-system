                                        $(".getClass").change(function() {
            var class_id = $(this).val();

            $.ajax({
                type: "get",
                url: "{{ url('admin/class_timetable/get_subject') }}",
                data: {
                    "_token": "{{ csrf_token() }}",
                    class_id:class_id,

                },
                datatypr:"json",
                success:function(){

                },
            });

        });
